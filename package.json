{
  "name": "torqle",
  "private": true,
  "version": "0.4.0",
  "type": "module",
  "os": ["darwin", "linux", "windows"],
  "cpu": ["x64", "arm64"],
  "scripts": {
    "start": "npm run tauri:dev",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "RUST_BACKTRACE=1 tauri dev",
    "tauri:icon": "npm run tauri icon",
    "tauri:test": "npm run cargo test ",
    "cargo": "cd ./src-tauri && cargo",
    "seed": "concurrently \"npm run seed:mongo\" \"npm run seed:mysql\" \"npm run seed:postgres\"",
    "seed:mongo": "node data/docker-seed-mongo.js",
    "seed:mysql": "node data/docker-seed-mysql.js",
    "seed:postgres": "node data/docker-seed-postgres.js",
    "test": "vitest"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@mdi/js": "^7.4.47",
    "@tauri-apps/api": "^2",
    "@tauri-apps/cli": "^2",
    "@tauri-apps/plugin-dialog": "^2",
    "@tauri-apps/plugin-fs": "^2",
    "@tauri-apps/plugin-global-shortcut": "^2",
    "@tauri-apps/plugin-os": "^2",
    "@tauri-apps/plugin-shell": "^2",
    "ace-builds": "^1.36.2",
    "ace-code": "^1.36.2",
    "crypto-js": "^4.2.0",
    "highlight.js": "^11.10.0",
    "monaco-editor": "^0.41.0",
    "pinia": "^2.2.2",
    "sass": "^1.79.4",
    "simple-code-editor": "^2.0.9",
    "uuid": "^10.0.0",
    "vue": "^3.5.10",
    "vuetify": "^3.7.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.3",
    "@vitejs/plugin-vue": "^5.1.4",
    "concurrently": "^9.0.1",
    "knex": "^3.1.0",
    "mongodb": "^6.9.0",
    "mysql2": "^3.11.3",
    "pg": "^8.13.0",
    "sqlite3": "^5.1.7",
    "vite": "^5.4.8",
    "vitest": "^2.1.1"
  }
}
